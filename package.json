{
  "name": "jsr",
  "version": "0.1.0",
  "description": "jsr.io package manager for node",
  "bin": {
    "jsr": "./dist/bin.js"
  },
  "main": "dist/index.js",
  "exports": {
    ".": {
      "import": "./dist-esm/index.js",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "test": "node -r ts-node/register --test **/*.test.ts",
    "cli": "ts-node src/bin.ts",
    "build": "rimraf dist dist-esm && tsc && tsc -p tsconfig.esm.json",
    "prepublishOnly": "tsc && npm test"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "files": [
    "dist/",
    "dist-esm/"
  ],
  "devDependencies": {
    "@types/node": "^20.11.18",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@std/encoding": "npm:@jsr/std__encoding@^0.216.0",
    "kolorist": "^1.8.0"
  }
}
